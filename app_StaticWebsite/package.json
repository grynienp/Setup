{
  "name": "staticwebsite_homebase",
  "version": "0.0.1",
  "description": "A Static Web Site scaffold for simple web sites.",
  "main": "./src/functionality/main.js",
  "scripts": {
    "dev:webpack":
      "webpack --progress --colors --config ./src/webpack/webpack.config.js --env.env=dev --env.platform=web",
    "dev:webpack-server":
      "webpack-dev-server --history-api-fallback --inline --progress --color --env.env=dev --env.platform=web --config ./src/webpack/webpack.config.js",
    "server":
      "nodemon --watch webpack.*.js --exec npm run \"dev:webpack-server\"",
    "build":
      "webpack --progress --colors --config ./src/webpack/webpack.config.js --env.env=dev --env.platform=web",
    "clean": "rimraf -rf ./dist/* ./docs/*",
    "share": "npm-run-all -p localtunnel",
    "security": "npx nsp check --reporter summary",
    "localtunnel":
      "npx lt --port 3001 --localhost NickGoganTunnel --subdomain nickgogan",
    "postinstall": "flow-scripts stub"
  },
  "keywords": [
    "starter",
    "kit",
    "guide",
    "boilerplate",
    "scaffold",
    "web",
    "site",
    "static",
    "webpack",
    "hmr",
    "hot",
    "babel"
  ],
  "author": "Nick Gogan <gogannick@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickgogan/Setup"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "0.1.0",
    "@types/extract-text-webpack-plugin": "3.0.0",
    "@types/html-webpack-plugin": "2.30.1",
    "@types/webpack": "3.8.2",
    "@types/webpack-dev-server": "2.9.2",
    "@types/webpack-merge": "4.1.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "clean-webpack-plugin": "0.1.17",
    "copy-webpack-plugin": "4.3.1",
    "css-hot-loader": "1.3.5",
    "css-loader": "0.28.9",
    "documentation": "5.3.5",
    "dotenv-safe": "4.0.4",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-html": "4.0.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "flow-bin": "0.62.0",
    "flow-scripts": "0.4.0",
    "html-webpack-harddisk-plugin": "0.1.0",
    "html-webpack-plugin": "2.30.1",
    "install-peerdeps": "1.4.1",
    "localtunnel": "1.8.3",
    "nodemon": "1.14.7",
    "nsp": "3.1.0",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.10",
    "precss": "3.0.0",
    "rimraf": "2.6.2",
    "style-loader": "0.19.1",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.0",
    "webpack-merge": "4.1.1",
    "webpack-node-externals": "1.6.0"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "console-log-html": "2.0.2",
    "dotenv-webpack": "1.5.4"
  },
