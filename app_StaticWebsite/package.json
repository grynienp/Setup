{
  "name": "staticwebsite_homebase",
  "version": "0.0.1",
  "description": "A Static Web Site scaffold for simple web sites.",
  "main": "./src/functionality/main.js",
  "scripts": {
    "debugDemo": "nodemon --inspect-brk=localhost:3001 ./src/demo_Debugging/app.js --exec babel-node",
    "dev:start": "npx cross-env NODE_ENV=dev npm-run-all -s security dev:start-app",
    "dev:start-app": "npx cross-env NODE_ENV=dev babel-node ./src/app/server/server.js",
    "dev:debug-app": "npx cross-env NODE_ENV=dev nodemon --inspect-brk=localhost:3001 ./src/app/app.js --exec babel-node",
    "dev:debug-server": "npx cross-env NODE_ENV=dev nodemon --inspect-brk=localhost:3001 ./src/app/server/server.js --exec babel-node",
    "prod:debug-app": "npx cross-env NODE_ENV=prod nodemon --inspect-brk=localhost:3001 ./dist/app.js",
    "prod:debug-server": "npx cross-env NODE_ENV=prod nodemon --inspect-brk=localhost:3001 ./dist/server/server.js",
    "start": "npm-run-all -s security publish start-app",
    "start-app": "npx cross-env NODE_ENV=prod node ./dist/server/server.js",
    "publish": "npm-run-all -s clean build document:html",
    "document:html": "npx documentation build ./src/app/* -f html -o docs",
    "build": "npx cross-env NODE_ENV=prod babel ./src/app/ --out-dir dist/ --source-maps",
    "clean": "rimraf -rf ./dist/* ./docs/*",
    "share": "npm-run-all -p localtunnel",
    "security": "npx nsp check --reporter summary",
    "localtunnel": "npx lt --port 3001 --localhost NickGoganTunnel --subdomain nickgogan",
    "postinstall": "flow-scripts stub"
  },
  "keywords": [
    "starter",
    "kit",
    "guide",
    "web",
    "site",
    "static",
    "webpack",
    "hmr"
  ],
  "author": "Nick Gogan",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "documentation": "5.3.5",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-html": "4.0.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "0.62.0",
    "flow-scripts": "0.4.0",
    "install-peerdeps": "1.4.1",
    "localtunnel": "1.8.3",
    "nodemon": "1.14.7",
    "nsp": "3.1.0",
    "rimraf": "2.6.2"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "cross-env": "5.1.3",
    "dotenv-safe": "4.0.4"
  }
}
