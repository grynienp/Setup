{
  "name": "staticwebsite_homebase",
  "version": "0.1.0",
  "description": "A Static Web Site scaffold for simple web sites.",
  "main": "./src/js/main.js",
  "scripts": {
    "prod": "npm-run-all -s prod:clean prod:build",
    "prod:build": "webpack --colors --config ./src/webpack/webpack.config.js --env.prod --env.platform=web",
    "prod:clean": "rimraf -rf ./dist/* ./docs/*",
    "server": "nodemon --watch ./src/webpack --ignore node_modules --exec npm run \"dev:server\"",
    "dev": "npm-run-all -s dev:clean dev:build",
    "dev:server": "webpack-dev-server --inline --color --env.dev --env.platform=web --config ./src/webpack/webpack.config.js",
    "dev:build": "webpack --progress --colors --config ./src/webpack/webpack.config.js --env.dev --env.platform=web",
    "dev:clean": "rimraf -rf ./build/* ./docs/*",
    "share": "npm-run-all -s security localtunnel",
    "security": "npx nsp check --reporter summary",
    "localtunnel": "npx lt --port 3001 --localhost NickGoganTunnel --subdomain nickgogan",
    "postinstall": "flow-scripts stub"
  },
  "keywords": [
    "starter",
    "kit",
    "guide",
    "boilerplate",
    "scaffold",
    "web",
    "site",
    "static",
    "webpack",
    "hmr",
    "hot",
    "babel"
  ],
  "author": "Nick Gogan <gogannick@gmail.com>",
  "contributors": [
    {
      "name": "Nick Gogan",
      "email": "gogannick@gmail.com"
    }
  ],
  "license": "ISC",
