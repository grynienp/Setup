	// Place your snippets for Plain Text here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
	"JIRA: Publishing Statement": {
		"prefix": "published",
		"body": "Published ${pub} on ${date} at ${time}",
		"description": "Template: dev publishing to prod."
	},
	"JIRA: Testing Statement": {
		"prefix": "testing",
		"body":
			"~~~~~~\nTesting\n\nPlease log in via ${logon}\nthrough QA-Atom: https://atom.qa.ktp.io/login/atom\nwith env/ settings: ${env}.\nSyllabus type: ${types}\nSyllabus version: ${versions}",
		"description": "Template: from dev to QA"
	}
}