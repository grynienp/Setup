  "scripts": {
    "deploy": "run-s prod:clean prod:build deploy-firebase",
    "deploy-firebase": "firebase deploy",
    "prod": "run-s prod:clean prod:build prod:serve",
    "prod:serve": "cross-env NODE_ENV=production nodemon --watch src/webpack/* --watch postcss.config.js --watch server/* --ignore node_modules --exec babel-node server",
    "prod:build": "webpack --progress --colors --config ./src/webpack/webpack.config.prod.babel.js",
    "prod:clean": "rimraf -rf ./dist/* ./docs/*",
    "dev": "run-s dev:clean dev:build dev:serve",
    "dev:serve": "cross-env NODE_ENV=development nodemon --watch src/webpack/* --watch postcss.config.js --watch server/* --ignore node_modules --exec babel-node server",
    "dev:build": "webpack --progress --colors --config ./src/webpack/webpack.config.dev.babel.js",
    "dev:clean": "rimraf -rf ./build/* ./docs/*",
    "share": "run-s security localtunnel",
    "security": "npx nsp check --reporter summary",
    "localtunnel": "npx lt --port 3001 --localhost NickGoganTunnel --subdomain nickgogan",
    "postinstall": "flow-scripts stub"
