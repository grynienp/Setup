{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "node_Attach",
      "port": 3001,
      "protocol": "inspector",
      "restart": true,
      "sourceMaps": true,
    },
    {
      "type": "node",
      "request": "launch",
      "name": "nodemon_Launch",
      "runtimeExecutable": "nodemon",
      "args": [
        "./src/main.js",
        "--exec",
        "babel-node"
      ],
      //"program": "${file}", //Go to the file you want to debug/js project root and then start this debugger
      //"localRoot": "${workspaceFolder}/src/main.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "protocol": "inspector",
      "skipFiles": [
        "node_modules/**/*.js"
      ],
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node_Launch(Client)",
      "program": "${workspaceFolder}/src/client.js"
    }
  ],
  "compounds": [
    {
      "name": "Fullstack",
      "configurations": [
        "nodemon_Launch",
        "Node_Launch(Client)"
      ]
    }
  ]
}